{
  "scenario": "evaluate",  // 1.evaluate,  2. optimize
  "function_type": 1,  // Types of functions: coco:1, normal:2
  "coco_id": 1,  // Set the function id
  "constrained": true,  // True if there are contraints
  "dimensions": 2,  // Input dimension âˆˆ {2,3,5,10,20,40}
  "points": 10,  // Number of points to be optimized
  "opt_read": false,  // True if xopt, fopt, ystar, x_init are read from data.csv file.
  "min_method": "COBYLA",  // Minimization Method: BFGS, COBYLA, SLSQP, ...
  "constraints": 3,  // Number of constraints
  "log_scale": true,  // Plot in log scale mode
  "grid_size": 200,  // Grid size for plotting in a 2d field: 10 -> 10x10

  // Lower and upper bounds in minimization process: -1 for dim=2 becomes [-1, -1] vector
  // Only for Nelder-Mead, L-BFGS-B, TNC, SLSQP, Powell, and trust-constr methods
  "lower_bounds": -10,
  "upper_bounds": 10
}
